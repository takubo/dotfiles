# A sample of ~/.w3m/keymap (default)
#
# Ctrl  : C-, ^
# Escape: ESC-, M-, ^[
# Space : SPC, ' '
# Tab   : TAB, ^i, ^I
# Delete: DEL, ^?
# Up    : UP, ^[[A
# Down  : DOWN, ^[[B
# Right : RIGHT, ^[[C
# Left  : LEFT, ^[[D


keymap	H	MOVE_LEFT
keymap	j	MOVE_DOWN
keymap	k	MOVE_UP
keymap	L	MOVE_RIGHT

keymap	g	BEGIN
keymap	G	END

keymap	J	UP
keymap	K	DOWN
keymap	SPC	NEXT_PAGE
keymap	b	PREV_PAGE

keymap	TAB	NEXT_LINK
#keymap	S-TAB	PREV_LINK	# force


keymap	h	PREV
keymap	l	NEXT
keymap	r	RELOAD


keymap	/	ISEARCH
keymap	?	ISEARCH_BACK
keymap	n	SEARCH_NEXT
keymap	N	SEARCH_PREV


keymap	C-l	REDRAW
keymap	q	QUIT
keymap	Q	ABORT
keymap	C-z	SUSPEND
keymap	R	VERSION
keymap	!	SHELL



keymap	C-@	MARK
keymap	C-a	LINE_BEGIN
keymap	C-b	MOVE_LEFT
keymap	C-e	LINE_END
